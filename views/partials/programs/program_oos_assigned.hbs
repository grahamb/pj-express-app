<hr>
<section class="oos_assigned">
    <h3>OOS Assigned</h3>
    <p>{{OOS.length}} of {{oos_required}} required</p>
    {{#if OOS.length}}
    <table>
        <thead>
            <tr>
                <th></th>
                <th>First Name</th>
                <th>Last Name</th>
                <th>Current Age</th>
                <th>Email</th>
            </tr>
        </thead>
        <tbody>
            {{#each OOS}}
            <tr>
                <td><a href="/oos/{{this.id}}">{{this.oos_number}}</a>{{{pal_icon this.id ../program_activity_leader_id}}}</td>
                <td>{{this.first_name}}</td>
                <td>{{this.last_name}}</td>
                <td>{{this.current_age}}</td>
                <td>{{this.email}}</td>
            </tr>
            {{/each}}
        </tbody>
    </table>
    <a class="button" href="/programs/{{id}}/oos/csv"><i class="fa fa-file-text-o"></i>Export OOS list as CSV</a>
    {{/if}}
</section>